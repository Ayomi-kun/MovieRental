@model MovieRentalApp.ViewModels.RandomMovieViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customers</h2>
@{ var num = 0;}

@foreach (var customer in Model.Customers)
{
    num++;
    <h5> @Html.ActionLink(customer.Name,"Details","Customers", new { id = num}, null)</h5>
    
}
